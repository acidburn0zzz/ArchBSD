pkgname=dri2proto
pkgver=2.6
pkgrel=1
category=x11
pkgdesc="DRI2 prototype headers"
arch=(any)
url="http://xorg.freedesktop.org"
license=('custom')

build() {
  cd "$portsdir/$category/$pkgname"
  make clean
  make
  make deinstall
  make package PACKAGES="$srcdir"
}

package() {
  cd $pkgdir
  tar xpf "${srcdir}/${category}/${pkgname}-${pkgver}.txz"
}


