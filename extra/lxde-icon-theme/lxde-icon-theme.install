post_install() {
    /usr/local/bin/gtk-update-icon-cache -q -f usr/local/share/icons/nuoveXT2 2>/dev/null || /usr/bin/true
}

post_upgrade() {
    /usr/local/bin/gtk-update-icon-cache -q -f usr/local/share/icons/nuoveXT2 2>/dev/null || /usr/bin/true
}

post_remove() {
    /usr/local/bin/gtk-update-icon-cache -q -f usr/local/share/icons/nuoveXT2 2>/dev/null || /usr/bin/true
}
