pkgname=xproto
pkgver=7.0.22
pkgrel=1
category=x11
pkgdesc="X11 protocol headers"
arch=(any)
url="http://xorg.freedesktop.org"
license=('custom')
depends=('pkgconf')

build() {
  cd "$portsdir/$category/$pkgname"
  make clean
  make
  make deinstall
  make package PACKAGES="$srcdir"
}

package() {
  cd $pkgdir
  tar xpf "${srcdir}/${category}/${pkgname}-${pkgver}.txz"
}


