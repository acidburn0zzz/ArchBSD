pkgname=libfontenc
pkgver=1.1.0
pkgrel=1
category=x11-fonts
pkgdesc="X11 font encoding library"
arch=('i686' 'x86_64')
url="http://xorg.freedesktop.org/"
license=('custom')
depends=('xproto' )
build() {

  cd "$portsdir/$category/$pkgname"
  make clean
  make 
  make deinstall
  make package PACKAGES="$srcdir"
}

package() {
  cd $pkgdir
  tar xpf "${srcdir}/${category}/${pkgname}-${pkgver}.txz"
}


