pkgname=libx11
_pkgname=libX11
pkgver=1.4.4
pkgrel=1
category=x11
pkgdesc="X11 client-side library"
arch=('i686' 'x86_64')
url="http://xorg.freedesktop.org"
license=('custom')
depends=('kbproto' 'libxcb')
makedepends=('bigreqsproto' 'inputproto' 'libcheck' 'libgcrypt' 'libgpg-error' 'libxml2' 'libxslt' 'python2' 'xcb-proto' 'xcmiscproto' 'xextproto' 'xf86bigfontproto' 'xorg-macros' 'xtrans')

build() {
  cd "$portsdir/$category/$_pkgname"
  make clean
  make 
  make deinstall
  make package PACKAGES="$srcdir"
}

package() {
  cd $pkgdir
  tar xpf "${srcdir}/${category}/${_pkgname}-${pkgver},${pkgrel}.txz"
}


